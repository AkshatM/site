{
	"main": "dist/_worker.js/index.js",
	"name": "site",
  "routes": [
    {
      "pattern": "akshatmahajan.com/*",
      "zone_name": "akshatmahajan.com"
    },
  ],
  "compatibility_date": "2025-12-31",
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist"
	},
	"observability": {
    "enabled": true
  }
}